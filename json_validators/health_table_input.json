    "id": {
      "type": "integer"
    },
    "registered_doctor": {
      "type": "string"
    },
    "has_asthma": {
      "type": "boolean"
    },
    "has_registered_disability": {
      "type": "boolean"
    }
  },
  "required": [
    "id",
    "registered_doctor",
    "has_asthma",
    "has_registered_disability"
  ],
  "additionalProperties": false,
  "minProperties": 4,
  "maxProperties": 4
}
